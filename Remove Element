class Solution {
    public int removeElement(int[] nums, int val){
       int i = 0;
       int j = nums.length - 1;
       while(i <= j){
        if(nums[i] == val){
            if(nums[j] == val){
                i--;
            }else{
                int k = nums[i];
                nums[i] = nums[j];
                nums[j] = k;
            }
            j--;
        }
        i++;
       }
       return i;
       }
}
